@model ResetDTO
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/_AuthoLayout.cshtml";
}
<div class="main-agileinfo">
	<div class="agileits-top">
		<div asp-validation-summary="All" class="text-danger"></div>
		<form action="ResetPassword" method="post">
			
			<input class="text" asp-for="NewPassword" placeholder="Password" required="">
			<input class="text w3lpass" asp-for="ConfirmedPassword" placeholder="Confirm Password" required="">
			<div class="wthree-text">
				
				
			<input type="submit" value="Submit">
		</form>
		<p>Don't have an Account? <a asp-action="SignIn"> Login Now!</a></p>
	</div>
</div>
@section Validation {
	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}



